{
  "name": "pprunner",
  "version": "0.4.1",
  "main": "lib/main.js",
  "types": "lib/main.d.ts",
  "repository": "akito0107/pprunner",
  "license": "apache-2.0",
  "author": {
    "name": "Akito Ito",
    "email": "akito0107@gmail.com"
  },
  "bin": {
    "puppeteer-runner": "lib/bin/cli.js"
  },
  "scripts": {
    "lint": "tslint ./src/**/*.ts ./src/*.ts",
    "lint:fix": "tslint --fix ./src/**/*.ts ./src/*.ts",
    "clean": "rimraf ./lib/*",
    "build": "yarn clean && tsc --outDir lib",
    "watch": "yarn clean && tsc --outDir lib --watch",
    "test": "jest",
    "test:e2e": "run-p test:e2e:start-app test:e2e:run-tests",
    "test:e2e:run-tests": "wait-port localhost:3000 && node ./lib/bin/cli.js -p e2e/cases -i images && curl -XGET localhost:3000/_kill",
    "test:e2e:run-tests:firefox": "wait-port localhost:3000 && node ./lib/bin/cli.js -b firefox -p e2e/cases -i images && curl -XGET localhost:3000/_kill",
    "test:e2e:start-app": "cd e2e; yarn start"
  },
  "devDependencies": {
    "@types/debug": "^4.1.2",
    "@types/faker": "^4.1.5",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.4",
    "@types/pino": "^5.8.4",
    "@types/puppeteer": "^1.12.3",
    "@types/selenium-webdriver": "^4.0.0",
    "express": "^4.16.4",
    "jest": "^24.4.0",
    "npm-run-all": "^4.1.5",
    "power-assert": "^1.6.1",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.3.4",
    "wait-port": "^0.2.2"
  },
  "dependencies": {
    "commander": "^2.19.0",
    "debug": "^4.1.1",
    "faker": "^4.1.0",
    "handlebars": "^4.1.0",
    "immer": "^3.0.0",
    "js-yaml": "^3.12.1",
    "p-iteration": "^1.1.8",
    "pino": "^5.11.1",
    "puppeteer": "^1.12.2",
    "puppeteer-firefox": "^0.5.0",
    "randexp": "^0.5.3",
    "recursive-readdir": "^2.2.2",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "source-map-support": "^0.5.12"
  }
}
